@model Recruitment.API.Models.Test

<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <h4><a asp-action="Index">Atpakaļ pie visiem kandidātiem</a></h4>
        </div>
    </div>
    <hr />

    <form asp-action="Create">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <label asp-for="Name" class="col-md-offset-1 col-md-3 col-form-label"></label>
            <div class="col-md-3">
                <input asp-for="Name" class="form-control" />
            </div>
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group pull-right">
            <input type="submit" value="Izveidot" class="btn btn-info" />
        </div>
    </form>
</div>

